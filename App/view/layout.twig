{# layout.twig #}
<!DOCTYPE HTML>
<html lang="en-US">
<head>
    {% include 'head.twig' %}
    {% block stylesheets %}
        <link rel="stylesheet" href="/vendor/bootstrap/css/bootstrap.min.css">
        <link rel="stylesheet" href="/vendor/bootstrap/css/bootstrap-responsive.min.css">
        <link rel="stylesheet" href="/css/style.css">
    {% endblock %}
    <script type="text/javascript" src="/vendor/jquery/jquery.js">
    </script>
</head>
<body>
{% include "main-nav.twig" %}
<div class="container">
    <div class="row">
        <noscript>
            <div class='alert-error'>
                <h1>Please enable Javascript to have the best experience with that website. thank you.</h1>
            </div>
        </noscript>
        <div class="span12">
            {% include 'flash-message.twig' %}
        </div>
    </div>
    {% block herounit %}
    {% endblock %}
    <div class="row">

        {% block left_sidebar %}
        {% endblock %}
        {% block content %}
        {% endblock %}
        {% block right_sidebar %}
            {% include 'sidebar.twig' %}
        {% endblock %}
    </div>
</div>
<div class='well'>
    {% include 'footer.twig' %}
</div>
<script type="text/javascript" src="/vendor/bootstrap/js/bootstrap.min.js"></script>
{% include 'analytics.twig' %}
</body>
</html>
